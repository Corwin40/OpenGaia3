<!DOCTYPE html>
<html>
<head>
    <script src="https://tarteaucitron.io/load.js?domain=www.justafaire.fr&uuid=9ea6989a24d427ca2686cf2a995496780a46283b"></script>
    {% block meta %}
    {{ render(controller('App\\Controller\\Webapp\\PublicController::meta')) }}
    {% endblock %}
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <title>{% block title %}OpenGaia{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.21.1/axios.min.js"></script>
        <script src="{{ asset('bundles/ckeditor5/build/ckeditor.js') }}"></script>



    {% endblock %}
</head>
<body>
<div class="wrapper">
{% block header %}
    {{ render(controller('App\\Controller\\Webapp\\PublicController::BlocMenu')) }}
{% endblock %}
    <main id="content">
        {% block content %}
        {% endblock %}
    </main>
    {% block footer %}
        {{ include('include/footer_webapp.html.twig') }}
    {% endblock %}
</div>

</body>
</html>