{{ form_start(form) }}
<div class="card">
    <div class="card-header">
        AJOUT DE MEDIAS
    </div>
    <div class="card-body">
        <form id="addimage" enctype="multipart/form-data">
            <div>
                {{ form_row(form.imageFile) }}
                {{ form_errors(form.imageFile) }}
            </div>

        </form>
    </div>
    <div class="card-footer">
        <button class="btn btn-sm btn-outline-secondary">{{ button_label|default('Ajouter') }}</button>
    </div>
</div>
{{ form_end(form) }}

